    "prettier": "^2.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-config-xo": "^0.32.1",
    "jest": "^26.4.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "boxen": "^4.2.0",
